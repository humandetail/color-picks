{
  "name": "color-picks",
  "private": false,
  "version": "0.2.2",
  "author": "humandetail",
  "keywords": [
    "color-picks",
    "color",
    "picker",
    "picks",
    "ui"
  ],
  "description": "A color picker ui for browser",
  "type": "module",
  "main": "dist/color-picks.js",
  "browser": "dist/color-picks.js",
  "module": "dist/color-picks.js",
  "files": [
    "dist/*"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/color-picks.js"
    },
    "./index.css": "./dist/style.css"
  },
  "unpkg": "dist/color-picks.js",
  "jsdelivr": "dist/color-picks.js",
  "homepage": "https://github.com/humandetail/color-picks",
  "repository": {
    "type": "git",
    "url": "https://github.com/humandetail/color-picks"
  },
  "bugs": {
    "url": "https://github.com/humandetail/color-picks/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@vitest/coverage-c8": "^0.25.4",
    "happy-dom": "^8.1.0",
    "release-it": "^15.5.1",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vitest": "^0.25.4"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.12"
  }
